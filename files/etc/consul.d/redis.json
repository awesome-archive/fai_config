{
  "service": {
    "ID": "redis1",
    "Name": "redis",
    "Tags": [
      "primary",
      "v1"
    ],
    "Port": 8000,
    "Meta": {
      "redis_version": "4.0"
    },
    "Check": {
      "DeregisterCriticalServiceAfter": "90m",
      "Args": ["/usr/local/bin/check_redis.py"],
      "HTTP": "http://localhost:5000/health",
      "Interval": "10s"
    },
    "Weights": {
      "Passing": 10,
      "Warning": 1
    }
  }
}
