{
  "ignition": {
    "config": {},
    "version": "2.0.0"
  },
  "networkd": {},
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa "
        ]
      }
     ]
  },
  "storage": {
    "files": [
     {
      "filesystem": "root",
      "path": "/etc/sysctl.d/99-sysctl.conf",
      "contents": { "source": "https://raw.githubusercontent.com/slpcat/fai_config/master/files/etc/sysctl.conf/PXEBASE" }
     },
     {
      "filesystem": "root",
      "path": "/etc/security/limits.conf",
      "contents": { "source": "https://raw.githubusercontent.com/slpcat/fai_config/master/files/etc/security/limits.conf/PXEBASE" }
     },
     {
      "filesystem": "root",
      "path": "/etc/docker/daemon.json",
      "contents": { "source": "https://raw.githubusercontent.com/slpcat/fai_config/master/coreos/daemon.json" }
     }
    ]
},
  "systemd": {
    "units": [
      {
        "enable": true,
        "name": "etcd2.service"
      }
    ]
  }
